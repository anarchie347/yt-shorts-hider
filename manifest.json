{
    "manifest_version": 2,
    "name": "YT shorts hider",
    "version": "1.0",
    "description": "removes YT shorts from suggestions",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": [
                "removeShorts.js"
            ]
        }
    ]
}